<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.HBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.kidprogramgame.Hyb.MainController"
            fx:id="borderPane">

    <center>

    </center>

    <bottom>
        <HBox>
            <Button fx:id="upButton" text="上" onAction="#up"/>
            <Button fx:id="downButton" text="下" onAction="#down"/>
            <Button fx:id="leftButton" text="左" onAction="#left"/>
            <Button fx:id="rightButton" text="右" onAction="#right"/>
            <Button fx:id="refreshButton" text="刷新" onAction="#restart"/>
            <Button fx:id="testButton" text="测试" onAction="#useExecuteCommandTest"/>
        </HBox>
    </bottom>

</BorderPane>
